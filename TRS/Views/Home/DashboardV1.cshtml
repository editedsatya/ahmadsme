@section Scripts {

    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/daterangepicker.js"></script>


    <script>

        function _FunSearch() {
            var date = $('#datetimes').val();
            console.log(status);
            $('#hdn_DateRange').val(date);
            // alert(date);
            $.post('/Home/GetDataByDateRange/', { Daterange: date }, function (data) {
                //PartialDiv
                $('#Div1').html(data);
            });
            };

        $(function () {

            var date = $('#datetimes').val();
            //$('#hdn_DateRange').val(date);
            $('input[name="datetimes"]').daterangepicker({

                timePicker: true,

                startDate: moment().startOf('hour'),
                endDate: moment().startOf('hour').add(16, 'hour'),

                ranges: {

                    'Today': [moment(), moment()],

                    'Yesterday': [moment().subtract(1, 'days'), moment().subtract(1, 'days')],

                    'Last 7 Days': [moment().subtract(6, 'days'), moment()],

                    'Last 30 Days': [moment().subtract(29, 'days'), moment()],

                    'This Month': [moment().startOf('month'), moment().endOf('month')],

                    'Last Month': [moment().subtract(1, 'month').startOf('month'), moment().subtract(1, 'month').endOf('month')]
                },

                locale: {

                    format: 'DD/MM/YYYY hh:mm A'
                }
            });
        });

        function GetApproved() {
            //$("#mdbody").load('/Reports/PrintRecipt?RRN=' + id);

            if (@Model.SuccessTranactionCount!= 0) {
                $("#SuucessTranactionModal").modal("show");
            }
            //$("#hdn_rrnID").val(id);
        }

        function GetFailed() {
            if (@Model.FailedTranactionCount!= 0) {
                $("#FailedTranactionModal").modal("show");
            }
        }

        function GetAll() {
            if (@Model.AllTranactionCount!= 0) {
                $("#AllTranactionModal").modal("show");
            }
        }

        function GetSettled() {
             if (@Model.SelttedTranactionCount!= 0) {
                $("#AllSettledModal").modal("show");
            }
        }
    </script>

